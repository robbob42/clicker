{
    "env": {
        "browser": true,
        "es6": true,
        "mocha": true
    },
    "extends": ["airbnb","react-app"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react-hooks"
    ],
    "rules": {
    }
}